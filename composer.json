{
    "name": "malinoto/ezrun-structure",
    "license": "MIT",
    "type": "project",
    "description": "The \"EZRun Standard Edition\" distribution",
    "authors": [
        {
            "name": "Malin Ivanov",
            "email": "m.ivanov@webxploit-bg.com"
        }
    ],
    "autoload": {
        "psr-0": { "EZRun": "config/", "models": "models/" }
    },
    "require": {
        "php": ">=5.4.16",
        "malinoto/ezrun": "~1.0.0",
        "symfony/yaml": "2.5.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/common": "2.4.*",
        "twig/extensions": "~1.0"
    },
    "extra": {
        "ezrun-core-dir": "core",
        "ezrun-web-dir": "web",
        "incenteev-parameters": {
            "file": "config/settings.yml"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*"
    }
}